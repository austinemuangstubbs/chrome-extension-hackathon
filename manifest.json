{
  "name": "Hello Extensions",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "hello.html"
  },
  "background": {
    "service_worker": "service-worker.js"
  },

  "permissions": ["scripting", "activeTab", "storage", "tabs","nativeMessaging"],
  "content_scripts": [
   {
     "matches": ["https://*.wikipedia.org/*"],
     "js": ["content.js"]
   }
 ]
}
